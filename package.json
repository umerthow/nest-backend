{
  "name": "umerthow-boilerplate-be",
  "version": "0.0.4",
  "description": "Nest Boilerplate",
  "engines": {
    "node": ">=16.13.x"
  },
  "main": "src/main.ts",
  "author": "Umerthow",
  "license": "ahmad.umar.byl@gmail.com",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "nest start -d -w",
    "start": "node dist/main",
    "test": "rimraf coverage && jest --collectCoverage",
    "test:watch": "jest --watch",
    "lint:fix": "eslint . --ext .ts --fix",
    "generate:modules": "nest g mo && nest g co && nest g s",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "migration:create": "yarn typeorm migration:create",
    "migration:run": "yarn typeorm migration:run -d src/config/migration.config.ts",
    "migration:revert": "yarn typeorm migration:revert -d src/config/migration.config.ts",
    "entity:create": "yarn typeorm entity:create",
    "seeder:create": "yarn typeorm migration:create",
    "seeder:run": "yarn typeorm migration:run -d src/config/seeder.config.ts"
  },
  "homepage": "https://github.com/umerthow/",
  "lint-staged": {
    "**/*.ts": "eslint --fix",
    "*.{ts,json}": "prettier --write"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^3.2.1",
    "@google-cloud/storage": "^6.9.0",
    "@hapi/accept": "^6.0.0",
    "@liaoliaots/nestjs-redis": "^9.0.3",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/testing": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/ioredis": "^4.28.10",
    "@types/lodash": "^4.14.186",
    "@types/morgan": "^1.9.3",
    "@types/passport-jwt": "^3.0.7",
    "@types/request-ip": "^0.0.37",
    "@types/string-template": "^1.0.2",
    "axios": "^1.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "dayjs": "^1.11.5",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "husky": "^8.0.1",
    "ioredis": "^5.2.3",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "public-ip": "^4.0.4",
    "query-string": "^7.1.1",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "string-template": "^1.0.0",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.10",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@types/jest": "^27.4.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "commitlint-config-jira": "^1.5.2",
    "commitlint-plugin-jira-rules": "^1.5.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-loops": "^0.3.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
